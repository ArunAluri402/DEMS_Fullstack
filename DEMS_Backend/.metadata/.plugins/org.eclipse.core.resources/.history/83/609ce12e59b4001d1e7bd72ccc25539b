package com.example.demo.Model;

import java.util.List;

public class Department {
    
    private int id;
    private String name;
    private List<Employee> employees;
    
    public Department() {
    }
    
    public Department(int id, String name, List<Employee> employees) {
        this.id = id;
        this.name = name;
        this.employees = employees;
    }
    
    // getters and setters
}
