

package com.example.demo.Model;

public class Employee {
    
    private int id;
    private String name;
    private int departmentId;
    
    public Employee() {
    }
    
    public Employee(int id, String name, int departmentId) {
        this.id = id;
        this.name = name;
        this.departmentId = departmentId;
    }
    
    // getters and setters
}
